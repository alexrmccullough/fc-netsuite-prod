<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
    <macrolist>
    </macrolist>
    <style type="text/css">* {
		}
		table {
			font-size: 9pt;
			table-layout: fixed;
		}

		.darkmode {
			background-color: black;
			color: white;
		}

		th {
			font-weight: bold;
			font-size: 8pt;
			vertical-align: middle;
            padding: 5px 6px 3px;
            background-color: #e3e3e3;
			color: #333333;
		}
		td {
            padding: 4px 6px;
        }
		td p { align:left }
		b {
			font-weight: bold;
			color: #333333;
		}
		table.header td {
			padding: 0;
			font-size: 10pt;
		}
		table.footer td {
			padding: 0;
			font-size: 8pt;
		}
		table.itemtable th {
			padding-bottom: 10px;
			padding-top: 10px;
		}
		table.body td {
			padding-top: 2px;
		}
		td.addressheader {
			font-size: 8pt;
			padding-top: 6px;
			padding-bottom: 2px;
		}
		td.address {
			padding-top: 0;
		}
		span.title {
			font-size: 28pt;
		}
		span.number {
			font-size: 16pt;
		}
		span.itemname {
			font-weight: bold;
			line-height: 150%;
		}
		hr {
			width: 100%;
			color: #d3d3d3;
			background-color: #d3d3d3;
			height: 1px;
		}
</style>
</head>
<#assign last_shipto = ''>
<#assign last_docnumber = ''>
<#assign record_counter = 1>
<body header="nlheader" header-height="10%" footer="nlfooter" footer-height="20pt" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
<#list results as result>

<#if (record_counter > 1)>
	<#if (result.entity != last_shipto) || (result.tranid != last_docnumber)>
		</table>
	</#if>
	<#if (result.entity != last_shipto)>
	<pbr></pbr>
	</#if>
</#if>
<#if (result.entity != last_shipto)>
	<table class="header" style="width: 100%;"><tr>
		<td rowspan="3"><#if companyInformation.logoUrl?length != 0><img src="${companyInformation.logoUrl}" style="float: left; margin: 7px" /> </#if></td>
		<td align="right"><span class="title">${result.entity}</span></td>
		</tr>
		<tr>
		<td align="right"><span class="">Shipping Address 1<br></br>Shipping Address 2<br></br>Shipping Address 3<br></br></span></td>
		</tr>
		<tr>
		<td align="right">${result.shipdate}</td>
		</tr>
		<tr>
		<td align="right" style="padding-top:6px; font-weight: bold;">Route: ${result.custbody_rd_so_route}</td>
		</tr>		
	</table>
</#if>
<#if (result.tranid != last_docnumber)>
	<table class="darkmode" style="width: 100%; margin-top: 10px; margin-bottom: 0px;">
		<tr>
			<td style="width: 15%; font-weight: 700">Sales Order: </td>
			<td>${result.tranid}</td>
		</tr>
	</table>
	<table class="itemtable" style="width: 100%; margin-top: 0px;"><!-- start items -->
		<thead>
			<tr>
				<th style="width:10%; align:right; ">Quantity</th>
				<th>Item Name</th>

			</tr>
		</thead>
</#if>


<tr>
	<td style="align: right; vertical-align:middle; font-weight: bold; font-size: 11pt;">${result.quantity}</td>
	<td><span class="itemname">${result.itemid}</span><br />${result.displayname}</td>
</tr>


<#assign last_shipto = result.entity>
<#assign last_docnumber = result.tranid>
<#assign record_counter++>
</#list>
</table>
</body>
</pdf>